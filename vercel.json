{
  "version": 2,
  "functions": {
    "api/bot.ts": {
      "memory": 1024,
      "maxDuration": 10
    },
    "api/index.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/bot", "dest": "api/bot.ts" },
    { "src": "/*", "dest": "api/index.ts" }
  ]
}
